# URL completa de conexão com PostgreSQL
# Formato: postgres://usuario:senha@host:porta/nome_do_banco
DATABASE_URL=#url do banco de dados aqui

# Secret usado pelo express-session
SESSION_SECRET=#secret aqui

# Porta do servidor Express
PORT=#porta aqui

# Configurações do EmailJS
EMAILJS_PUBLIC=#public key aqui
EMAILJS_PRIVATE=#private key aqui
EMAILJS_SERVICE=#service id aqui
EMAILJS_TEMPLATE=#template id aqui